<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:myui='urn:import:com.baovue.myhobbyproject.client.application.ui'
	xmlns:dp="urn:import:com.google.gwt.user.datepicker.client" >

	<ui:with field="res" type="com.baovue.myhobbyproject.client.resources.AppResources" />

	<g:HTMLPanel>
		<myui:Header></myui:Header>
		<div class="{res.style.container}">
			<h2>Cooking</h2>
			<div class="{res.style.container} {res.style.box}">
	            <p>
	                <strong>Name</strong>
	                <g:TextBox ui:field="nameField"/>
	            </p>
	            <p>
	                <strong>Started</strong>
	                <dp:DateBox ui:field="startedDateField"></dp:DateBox>
	            </p>
	            <g:Button ui:field="resetButton">Reset Form</g:Button>
	            <g:HTML ui:field="error" styleName="{res.style.labelError}"/>
	        </div>
		</div>
	</g:HTMLPanel>
	

</ui:UiBinder> 